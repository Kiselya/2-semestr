#include <iostream>
int main()
{
    using namespace std;
    unsigned long long WorldPopulation, PopulationOfCountry;
    cout << "Enter World Population: "; 
    cin >> WorldPopulation;
    cout << "Introduce population Of Country: ";
    cin >> PopulationOfCountry;
    long double Percent = (long double) PopulationOfCountry / WorldPopulation * 100;
    cout << "Population is " << Percent << "% World Population.\n";
    return 0;
}
